@model SliderInfos
@{
    ViewData["Title"] = "Detail";
}

<div class="container mt-5">
    <h4 >Signature:</h4>
    <div class="signature">
        <img style="width:100px; height:100px" src="~/img/@Model.Signature" />
    </div>
    <h4 class="my-3">Title:</h4>
     <p>@Model.Title</p>
    <h4 class="my-3">Description:</h4>
    <p>@Model.Description</p>
    <a class="btn btn-outline-info mt-3" asp-action="Index">Back</a>

</div>

